"""
Module with elementary algorithms.
"""
from fern.algorithm import execution_policy
import _fern_algorithm_algebra_elementary as ext


def add(
        argument1,
        argument2):
    """
    Add the first argument to the second and return the result.

    Supported value types:

    - int64
    - double

    Supported data types:

    - number
    - raster
    """
    return ext.add(execution_policy(), argument1, argument2)


def iadd(
        argument1,
        argument2):
    """
    Add the first argument to the second and return the updated first
    argument.

    Supported value types:

    - int64
    - double

    Supported data types:

    - number
    - raster
    """
    return ext.iadd(execution_policy(), argument1, argument2)


