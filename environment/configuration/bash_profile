# vim:syntax=sh

export DEVENV_BRANCH=master

_devenv="$PROJECTS/`ls $PROJECTS | grep -i \"^devenv$\"`"
_personal_configuration="$_devenv/configuration/personal/kor"
unset _devenv

source "$_personal_configuration/profiles/common" $1
source "$_personal_configuration/profiles/DevEnv"
unset _personal_configuration

export RANALLY=$projects/Ranally

_basename=`basename $RANALLY`
export PATH=$RANALLY/environment/scripts:$OBJECTS/$_basename/bin:$PATH
unset _basename

cd $RANALLY
setPromptForProject Ranally $BUILD_TYPE
