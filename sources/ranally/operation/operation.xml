  <Operation>
    <Name>add</Name>
    <Description>Calculate the sum of the arguments.</Description>
    <Parameters>
      <Parameter>
        <Name>parameter1</Name>
        <Description>Parameter.</Description>
        <DataTypes>
          <DataType>Scalar</DataType>
        </DataTypes>
        <ValueTypes>
          <ValueType>Number</ValueType>
        </ValueTypes>
      </Parameter>
      <Parameter>
        <Name>parameter2</Name>
        <Description>Parameter.</Description>
        <DataTypes>
          <DataType>Scalar</DataType>
        </DataTypes>
        <ValueTypes>
          <ValueType>Number</ValueType>
        </ValueTypes>
      </Parameter>
    </Parameters>
    <Results>
      <Result>
        <Name>result</Name>
        <Description>The sum of the arguments.</Description>
        <DataType>DependsOnInput</DataType>
        <ValueType>DependsOnInput</ValueType>
      </Result>
    </Results>
  </Operation>
  <Operation>
    <Name>count</Name>
    <Description>Sum the number of feature elements in a feature.</Description>
    <Parameters>
      <Parameter>
        <Name>feature</Name>
        <Description>Feature.</Description>
        <DataTypes>
          <DataType>Feature</DataType>
        </DataTypes>
        <ValueTypes/>
      </Parameter>
    </Parameters>
    <Results>
      <Result>
        <Name>result</Name>
        <Description>Sum of feature elements.</Description>
        <DataType>Scalar</DataType>
        <ValueType>Size</ValueType>
      </Result>
    </Results>
  </Operation>
  <Operation>
    <Name>read</Name>
    <Description>Read a feature from a dataset.</Description>
    <Parameters>
      <Parameter>
        <Name>name</Name>
        <Description>Dataset name.</Description>
        <DataTypes>
          <DataType>Scalar</DataType>
        </DataTypes>
        <ValueTypes>
          <ValueType>String</ValueType>
        </ValueTypes>
      </Parameter>
    </Parameters>
    <Results>
      <Result>
        <Name>result</Name>
        <Description>Feature read.</Description>
        <DataType>Feature</DataType>
        <ValueType>All</ValueType>
      </Result>
    </Results>
